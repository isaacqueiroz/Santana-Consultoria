<h1>Elaborador de Testes</h1>

<% form_for(@test_tab) do |f| %>
  <%= error_messages_for 'test_tab', :object_name => "Teste" %>

  <p>
    <%= f.label :nome %><br />
    <%= f.text_field :nome %>
  </p>
  <p>
    <%= f.label :descrição %><br />
    <%= f.text_area :descricao %>
  </p>
  <p>
    <%= f.submit 'Elaborar' %>
  </p>
<% end %>

<%= link_to 'Voltar', test_tabs_path %>