<h1>Elaborador de Questões</h1>
<h2>Teste: <%=h @test_tab.nome%></h2>

<% form_for([@test_tab,@question_tab], :url => { :action => 'create' }, :html => { :multipart => true}) do |f| %>
  <%= error_messages_for 'question_tab', :object_name => "Questão" %>

  <p>
    <%= f.label :título %><br />
    <%= f.text_field :titulo %>
  </p>
  <p>
    <%= f.label :texto %><br />
    <%= f.text_area :texto %>
  </p>
  <p>
  	<%= f.label :imagem %>
  	<%= f.file_field :picture%>	
  </p>
  <p>
    <%= f.submit 'Elaborar' %>
  </p>
<% end %>

<%= link_to 'Voltar', test_tab_question_tabs_path(@test_tab) %>